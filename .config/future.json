{
    "watch": "./",
    "exec": [
        {
            "name": "python",
            "command": "python commands/server.py"
        },
        {
            "name": "backend-service",
            "command": "node index.js"
        }
    ],
    "on_events_only": true,
    "recursive": true,
    "patterns": [".rs", ".py", ".js"]
}